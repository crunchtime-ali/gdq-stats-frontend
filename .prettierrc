{
  "useTabs": false,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSameLine": false,
  "singleAttributePerLine": true,
  "printWidth": 100,
  "plugins": ["prettier-plugin-svelte"],
  "pluginSearchDirs": ["."],
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte"
      }
    }
  ]
}
